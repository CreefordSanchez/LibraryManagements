@model Book;

<h1>Add new Book</h1>

<form asp-action="CreateBook">
	<label asp-for="Title">Title: </label>
	<input asp-for="Title"/>

	<label asp-for="Author">Author: </label>
	<input asp-for="Author"/>

	<label asp-for="Genre">Genre: </label>
	<input asp-for="Genre"/>

	<label asp-for="Published">Published: </label>
	<input asp-for="Published" />

		
	<button type="submit">Post</button>
</form>